<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Media/layout.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-3.2.1.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/DayPilot/daypilot-all.min.js")" type="text/javascript"></script>
</head>

<body>
        <div id="header">
			<div class="bg-help">
				<div class="inBox">
					<h1 id="logo"><a href='http://code.daypilot.org/32389/asp-net-mvc-hotel-room-booking'>ASP.NET MVC Hotel Room Booking</a></h1>
					<p id="claim"><a href="http://mvc.daypilot.org/">DayPilot for ASP.NET MVC</a> - AJAX Calendar/Scheduling Controls for ASP.NET MVC</p>
					<hr class="hidden" />
				</div>
			</div>
        </div>
        <div class="shadow"></div>
        <div class="hideSkipLink">
        </div>
        <div class="main">
            @RenderBody()
        </div>
        <div class="clear">
        </div>
</body>
</html>
